        "n = 6\n",
        "sum=0\n",
        "for i in range(1,n):\n",
        "  if(n%i==0): # n=6 i=4\n",
        "    sum += i   #sum = 6\n",
        "\n",
        "if(n == sum):\n",
        "  print(\"Perfect\")\n",
        "else:\n",
        "  print(\"Not Perfect\")"
      ]
